<Project>
  <Import Project="$([MSBuild]::GetPathOfFileAbove(Directory.Build.props))" Condition="Exists('$([MSBuild]::GetPathOfFileAbove(Directory.Build.props))')" />
  <PropertyGroup>
    <ToolkitComponentName>MarkdownTextBlock</ToolkitComponentName>
  </PropertyGroup>

  <!-- Sets this up as a toolkit component's sample project -->
  <Import Project="$(ToolingDirectory)\ToolkitComponent.SampleProject.props" />
  <ItemGroup>
    <Content Remove="StreamingSamplePage.xaml.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Assets\MarkdownTextBlock.png" />
  </ItemGroup>
  <ItemGroup>
    <Page Remove="StreamingSamplePage.xaml" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Assets\MarkdownTextBlock.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <Content Update="StreamingSamplePage.xaml">
      <SubType>Designer</SubType>
      <Generator></Generator>
    </Content>
  </ItemGroup>
</Project>
